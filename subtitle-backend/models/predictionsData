// models/ActivityData.js
import { DataTypes } from 'sequelize';
import { sequelize } from '../db/index.js';

const predictionsData = sequelize.define('predictionsData', {
  id: {
    type: DataTypes.INTEGER,
    primaryKey: true,
    autoIncrement: true,
  },
  pred_list: DataTypes.TEXT,
  
}, {
  tableName: 'predictions',
  timestamps: false,
});

export default predictionsData;
